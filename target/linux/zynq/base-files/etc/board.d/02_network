#!/bin/sh
# Copyright (C) 2015 OpenWrt.org

. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
digilent,zynq-zybo | \
xlnx,zynq-zc702 | \
xlnx,zynq-zed)
	ucidef_set_interface_lan 'eth0'
	;;
*)
	echo "Unsupported hardware. Network interfaces not intialized"
	;;
esac

board_config_flush

exit 0
